    <script> 
      async function populateData() {
        const res = await fetch("http://localhost:5000/get_reccomendations?uid=2")
          .catch(e => console.error(e));

        const data = await res.json();
        console.log({data})
        const { git, /* interests,*/ mutuals } = data;

        document.getElementById("person1").innerHTML = "Jane Doe" // git.name;
        document.getElementById("caption1").innerHTML = "Junior Software Engineer<br>github.com/ethanrange" // `${git.title}<br>@${git.github}`;
        document.getElementById("acordian1").innerHTML = "You both code in Java" // `You both code in ${git.con.join(", ")}`;
        
        document.getElementById("person2").innerHTML = mutuals.name;
        document.getElementById("caption2").innerHTML = `${mutuals.title}<br>@${mutuals.github}`;
        document.getElementById("acordian2").innerHTML = `You both know ${mutuals.con.join(", ")}`;
        
        // document.getElementById("person3").innerHTML = interests.name;
        // document.getElementById("caption3").innerHTML = `${interests.title}<br>@${interests.github}`;
        // document.getElementById("acordian3").innerHTML = `You both code in ${interests.con.join(", ")}`;
      }
